{
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    {
      "src": "Fit-tracker/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Fit-tracker/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Fit-tracker/$1"
    }
  ]
}

